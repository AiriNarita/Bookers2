
<h1>Edit Book</h1>
  <%= form_with model: @book do |f| %>

    <h4>title</h4>
      <%= f.text_field :title %>

    <h4>opinion</h4>
      <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>

    <%= link_to "Show", book_path(@book), method: :get %>
    <%= link_to "Back", books_path, method: :get %>
  <% end %>