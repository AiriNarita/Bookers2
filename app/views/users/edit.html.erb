
<h1>Edit User</h1>
  <%= form_with model: @user do |f| %>

    <h4>title</h4>
      <%= f.text_field :name %>
    <h4>Image<h4>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

    <h4>introduction</h4>
      <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>
  <% end %>