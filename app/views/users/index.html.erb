
 <%= render partial: "partial/user_show", locals: { user: current_user }%>

 <%= render partial: "partial/book_new", locals: { book: @book } %>

<h2>Users</h2>
  <table>

    <thead>
      <tr>
         <th>Image</th>
         <th>Name</th>
         <th></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
       <tr>
         <th> <%= image_tag user.get_profile_image(100,100) %> </th>
         <th> <%= user.name %></th>
         <th> <%= link_to "Show", user_path(user.id), method: :get %></th>
       </tr>
     <% end %>
    </tbody>
  </table>